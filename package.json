{
  "name": "prompt-of-troy",
  "private": true,
  "version": "1.0.0",
  "description": "Prompt of Troy discord bot",
  "main": "dist/app.js",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "start": "node dist/app.js",
    "start:prod": "NODE_ENV=prod node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "discord:register": "ts-node src/discordInstall/commands.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "exports": {
    "./*": "./dist/*"
  },
  "author": "Shay DeWael",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.47.3",
    "discord-interactions": "^4.0.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.2",
    "express": "^4.21.2",
    "openai": "^4.76.0",
    "postgres": "^3.4.5", 
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@types/jest": "^29.5.12",
    "drizzle-kit": "^0.30.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2"
  }
}
