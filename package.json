{
  "name": "prompt-of-troy",
  "private": true,
  "version": "1.0.0",
  "description": "Prompt of Troy discord bot",
  "main": "dist/app.js",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "start": "node dist/src/app.js",
    "start:prod": "NODE_ENV=prod node dist/src/app.js",
    "dev": "NODE_ENV=prod nodemon",
    "build:server": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:frontend": "vite build",
    "build": "rm -rf dist && concurrently \"npm run build:server\" \"npm run build:frontend\"",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "discord:register": "ts-node src/discordInstall/commands.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev:frontend": "vite",
    "preview:frontend": "vite preview"
  },
  "exports": {
    "./*": "./dist/*"
  },
  "author": "Shay DeWael",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.47.3",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "discord-interactions": "^4.0.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.2",
    "express": "^4.21.2",
    "openai": "^4.76.0",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "drizzle-kit": "^0.30.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "vite": "^5.0.0"
  }
}
